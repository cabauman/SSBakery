            xmlns:ui="clr-namespace:SSBakeryAdmin.UI.Common"
            xmlns:modules="clr-namespace:SSBakeryAdmin.UI.Modules"
            x:TypeArguments="modules:ISignInViewModel"
            x:Class="SSBakeryAdmin.UI.Modules.SignInPage">
    <ContentPage.Content>
        <Button
            x:Name="GoogleSignInButton"
